#include <stdio.h>
#include <math.h>
int main() {
    int n;
    double sum = 0;
    printf("Enter a positive integer n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; ++i) {
        sum += pow(i, i);
    }
    printf("The sum of the series 1^1 + 2^2 + 3^3 + ... + %d^%d is: %.0f\n", n, n, sum);
    return 0;
}
